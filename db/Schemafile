create_table 'users', force: :cascade do |t|
  t.string   'name'
  t.string   'image'
  t.string   'email'
  t.boolean  'admin_flag', default: false, null: false
  t.string   'last_login_provider'
  t.datetime 'last_login_at'
  t.text     'user_agent'

  t.datetime 'created_at'
  t.datetime 'updated_at'
end

create_table 'authentications', force: :cascade do |t|
  t.integer 'user_id'
  t.string  'provider'
  t.string  'uid'
  t.string  'name'
  t.string  'nickname'
  t.string  'image'
  t.string  'email'
  t.string  'location'
  t.string  'gender'
  t.string  'token'
  t.string  'secret'

  t.datetime 'created_at'
  t.datetime 'updated_at'

  t.index :user_id
end
